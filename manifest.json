{
    "manifest_version": 3,
    "name": "arxiv-test",
    "version": "0.1.0",
    "description": "A Chrome extension for testing arXiv DOM analysis",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://arxiv.org/abs/*"],
    "action": {
        "default_popup": "popup.html"
    }
}